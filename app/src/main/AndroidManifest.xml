<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.guia7labmoviles">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".configUsuario"
            android:label="@string/label_Configuracion"></activity>
        <activity
            android:name=".misDatos"
            android:label="@string/label_MisDatos" />
        <activity
            android:name=".verRespuestaCorrecta"
            android:label="@string/label_Respuesta" />
        <activity
            android:name=".verPuntaje"
            android:label="@string/label_Puntaje" />
        <activity
            android:name=".jugar"
            android:label="@string/label_Jugar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>